#!/bin/sh
#
# Pre installation script for GRR client MacOS-X package

if [ -f "%(ClientBuildDarwin.old_plist_path)" ];
then
  sudo launchctl unload "%(ClientBuildDarwin.old_plist_path)";
  sudo rm "%(ClientBuildDarwin.old_plist_path)";
fi

if [ -f "%(ClientBuildDarwin.plist_path)" ];
then
  sudo launchctl unload "%(ClientBuildDarwin.plist_path)";
fi

